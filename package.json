{
  "name": "cli-helpers",
  "version": "0.1.0",
  "main": "./index.js",
  "scripts": {
    "test": "npm run mochaTest",
    "mochaTest": "mocha --compilers js:babel-register --timeout 20000 --slow 350",
    "watch": "nodemon --exec 'npm run mochaTest' --watch index.js --watch test --ext js",
    "lint": "eslint **/*.js --quiet",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-polyfill": "6.7.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "eslint": "2.2.0",
    "expect": "1.15.2",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "shelljs": "0.6.0",
    "underscore": "1.8.3",
    "yargs": "4.2.0"
  }
}
