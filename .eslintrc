{
  "parser": "babel-eslint",
  "rules": {
    "strict": [2, "global"],
    "no-console": [2, { allow: ["error", "info"] }],
    "semi": [2, "never"],
    "eol-last": [0],
    "quotes": [2, "single"],
    "no-underscore-dangle": [0],
    "no-use-before-define": [0]
  },
  "extends": ["eslint:recommended"]
}
